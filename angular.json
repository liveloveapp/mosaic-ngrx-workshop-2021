{
  "version": 1,
  "projects": {
    "books-page": {
      "projectType": "library",
      "root": "libs/books-page",
      "sourceRoot": "libs/books-page/src",
      "prefix": "bco",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/books-page/src/**/*.ts",
              "libs/books-page/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/books-page"],
          "options": {
            "jestConfig": "libs/books-page/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "books-page-actions": {
      "root": "libs/books-page/actions",
      "sourceRoot": "libs/books-page/actions/src",
      "projectType": "library",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/books-page/actions/**/*.ts"]
          }
        }
      }
    },
    "earnings": {
      "projectType": "application",
      "root": "apps/earnings",
      "sourceRoot": "apps/earnings/src",
      "prefix": "book-co",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/earnings",
            "index": "apps/earnings/src/index.html",
            "main": "apps/earnings/src/main.ts",
            "polyfills": "apps/earnings/src/polyfills.ts",
            "tsConfig": "apps/earnings/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "apps/earnings/src/favicon.ico",
              "apps/earnings/src/assets"
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/deeppurple-amber.css",
              "apps/earnings/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "apps/earnings/src/environments/environment.ts",
                  "with": "apps/earnings/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "earnings:build:production"
            },
            "development": {
              "browserTarget": "earnings:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "earnings:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/earnings/src/**/*.ts",
              "apps/earnings/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/earnings"],
          "options": {
            "jestConfig": "apps/earnings/jest.config.js",
            "passWithNoTests": true
          }
        },
        "backend": {
          "builder": "@nrwl/workspace:run-commands",
          "outputs": [],
          "options": {
            "command": "json-server db.json --watch"
          }
        }
      }
    },
    "shared-models": {
      "projectType": "library",
      "root": "libs/shared-models",
      "sourceRoot": "libs/shared-models/src",
      "prefix": "book-co",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/shared-models/src/**/*.ts",
              "libs/shared-models/src/**/*.html"
            ]
          }
        }
      }
    },
    "shared-services": {
      "projectType": "library",
      "root": "libs/shared-services",
      "sourceRoot": "libs/shared-services/src",
      "prefix": "book-co",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/shared-services/src/**/*.ts",
              "libs/shared-services/src/**/*.html"
            ]
          }
        }
      }
    },
    "shared-state-books": {
      "projectType": "library",
      "root": "libs/shared-state-books",
      "sourceRoot": "libs/shared-state-books/src",
      "prefix": "bco",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/shared-state-books/src/**/*.ts",
              "libs/shared-state-books/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/shared-state-books"],
          "options": {
            "jestConfig": "libs/shared-state-books/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "schematics": {
    "@nrwl/angular": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      },
      "storybook-configuration": {
        "linter": "eslint"
      }
    },
    "@nrwl/angular:application": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:component": {
      "style": "scss"
    }
  },
  "defaultProject": "earnings"
}
